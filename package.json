  "author": "Team 15",
  "private": true,
  "devDependencies": {
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "scripts": {
    "lint": "npx eslint . --ext .js",
    "install-dep": "npm install && run-s install-all:*",
    "install-all:gateway": "cd api-gateway && npm install",
    "install-all:coding": "cd coding-service && npm install",
    "install-all:frontend": "cd frontend && npm install",
    "install-all:matching": "cd matching-service && npm install",
    "install-all:question": "cd question-service && npm install",
    "install-all:user": "cd user-service && npm install",
    "install-all:video": "cd video-service && npm install",
    "start": "docker-compose up --build -d",
    "stop": "docker-compose down"
  }
}
